{
    "plugins": [
        "jtf-style"
    ],
    "rules": {
        "jtf-style/2.1.6.カタカナの長音": false,
        "jtf-style/4.2.1.感嘆符(！)": false,
        "max-ten": {
            "max": 3
        },
        "spellcheck-tech-word": true,
        "no-doubled-joshi": {
            "min_interval" : 2,
            "strict": true
        },
        "no-mix-dearu-desumasu": true,
        "sentence-length": {
            "max": 100
        }
    }
}
